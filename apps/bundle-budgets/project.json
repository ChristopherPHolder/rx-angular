{
  "projectType": "application",
  "generators": {},
  "root": "apps/bundle-budgets",
  "sourceRoot": "apps/bundle-budgets/src",
  "prefix": "bdgt",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/bundle-budgets",
        "index": "apps/bundle-budgets/src/index.html",
        "main": "apps/bundle-budgets/src/main.ts",
        "tsConfig": "apps/bundle-budgets/tsconfig.app.json",
        "vendorChunk": false,
        "extractLicenses": true,
        "buildOptimizer": true,
        "optimization": true,
        "sourceMap": false,
        "namedChunks": true,
        "budgets": [
          {
            "type": "bundle",
            "name": "main",
            "maximumWarning": "1kb"
          },
          {
            "type": "bundle",
            "name": "apps_bundle-budgets_src_empty-bundle_ts",
            "maximumWarning": "1b"
          }
        ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/bundle-budgets/src/environments/environment.ts",
              "with": "apps/bundle-budgets/src/environments/environment.prod.ts"
            }
          ]
        }
      },
      "outputs": ["{options.outputPath}"]
    }
  },
  "tags": []
}
